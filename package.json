  "scripts": {
    "start": "NODE_ENV=development node_modules/.bin/webpack && NODE_ENV=development node_modules/.bin/webpack-dev-server",
    "lint": "node_modules/.bin/eslint . --ext .vue,.js",
    "build": "NODE_ENV=production node_modules/.bin/webpack",
    "code-check": "NODE_ENV=development eslint .",
    "version": "yarn version:patch",
    "version:patch": "mversion patch",
    "version:minor": "mversion minor",
    "version:major": "mversion major",
